<div class="pure-g menu-bar item-set-modifiers">
  <div class="pure-u-1-5"></div>
  <div class="pure-u-1-5">
    <ul class="no-indent map-selector">
      <li data-pos="1" data-map="any">Any Map</li>
      <li data-pos="2" data-map="SR">Summoner's Rift</li>
      <li data-pos="3" data-map="TT">Twisted Treeline</li>
      <li data-pos="4" data-map="HA" class="unviable">Howling Abyss</li>
      <li data-pos="5" data-map="CS" class="unviable">Crystal Scar</li>
    </ul>
  </div>
  <div class="pure-u-1-5"></div>
  <div class="pure-u-1-5">
    <ul class="no-indent game-type-selector">
      <li data-pos="1" data-game-type="any">Any Mode</li>
      <li data-pos="2" data-game-type="CLASSIC">Classic</li>
      <li data-pos="3" data-game-type="ARAM">ARAM</li>
      <li data-pos="4" data-game-type="ODIN" class="unviable">Dominion</li>
    </ul>
  </div>
</div>

<div class="pure-g menu-bar champion-select">
  <div class="pure-u-1-6"></div>
  <div class="pure-u-2-3">
    <!-- <input type="text" placeholder="Champion"> -->
    <div style="color:red;font-style:italic;">
      Due to issues with the League API, some data may be inaccurate or unavailable.
    </div>
  </div>
</div>

<div class="pure-g main-container">
  <div class="pure-u-1-24"></div>
  <div class="pure-u-1-3">
    <div class="items">
      <input type="text" id="item-search" placeholder="Item">
      <%=
      erb :'partials/images/items/_index', locals: {items: get_items}
      %>
    </div>
  </div>
  <div class="pure-u-1-24"></div>
  <div class="pure-u-1-3">
    <div class="item-set-container">
      <input type="text" class="item-set-name" placeholder="Set Name">
      <div class="row-container"></div>
      <div class='item-set-row new-row'>+</div>
    </div>
  </div>
  <div class="pure-u-1-24"></div>
  <div class="pure-u-1-6">
    <div class="save-div">
      <button class="compile-button">Generate Link!</button><br><br>
      <!-- <button class="save-button">Save!</button> -->
    </div>
  </div>
</div>

