<div class="pure-g menu-bar item-set-modifiers">
  <div class="pure-u-1-4">
    <ul class="no-indent map-selector">
      <li data-pos="1" data-map="any">Any Map</li>
      <li data-pos="2" data-map="SR">Summoner's Rift</li>
      <li data-pos="3" data-map="HA">Howling Abyss</li>
      <li data-pos="4" data-map="TT">Twisted Treeline</li>
      <li data-pos="5" data-map="CS">Crystal Scar</li>
    </ul>
  </div>
  <div class="pure-u-1-8"></div>
  <div class="pure-u-1-4">
    <ul class="no-indent game-type-selector">
      <li data-pos="1" data-map="any">Any Mode</li>
      <li data-pos="2" data-map="CLASSIC">Classic</li>
      <li data-pos="3" data-map="ARAM">ARAM</li>
      <li data-pos="4" data-map="ODIN">Dominion</li>
    </ul>
  </div>
  <div class="pure-u-1-8"></div>
  <div class="pure-u-1-4">
    <input type="text" class="item-set-name" placeholder="Set Name">
  </div>
</div>

<div class="pure-g menu-bar champion-select">
  <div class="pure-u-1-6"></div>
  <div class="pure-u-2-3">
    <input type="text" placeholder="Champion">
  </div>
  <!-- <div class="pure-u-1-6"></div> -->
</div>

<div class="pure-g main-container">
  <div class="pure-u-1-3 items">
    <%=
    erb :'multi_page/images/items/index', locals: {items: get_items}
    %>
  </div>
  <div class="pure-u-1-3"></div>
  <div class="pure-u-1-3 item_set">
    <%= erb :'single_page/_item_set' %>
  </div>
</div>

